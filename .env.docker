# Docker Environment Variables
# Copy this file to .env and update with your actual values

# MySQL Configuration
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=AI_Agent_db
MYSQL_USER=aiagent
MYSQL_PASSWORD=aiagentpass

# Redis Configuration
REDIS_PASSWORD=redispass

# API Keys
GROQ_API_KEY=your_groq_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# JWT Configuration
JWT_SECRET_KEY=MySecretKeyForJWTTokenGenerationAndValidation123456789

# Service URLs (for production, update these)
NEXT_PUBLIC_API_URL=http://localhost:8089/api/v1
NEXT_PUBLIC_PYTHON_API_URL=http://localhost:5000
